<?xml version="1.0" encoding="UTF-8"?> 
 <definitions 
 xmlns="http://schemas.xmlsoap.org/wsdl/" 
 xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" 
 xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
 xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" name="ServerPollingService" targetNamespace="http://LoadBalancer/" xmlns:tns="http://LoadBalancer/" xmlns:wsp="http://www.w3.org/ns/ws-policy" xmlns:wsu="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-utility-1.0.xsd" xmlns:wsam="http://www.w3.org/2007/05/addressing/metadata" xmlns:wsrm="http://docs.oasis-open.org/ws-rx/wsrmp/200702" 
 >
    <message name="hello"/>
    <message name="helloResponse"/>
    <portType name="ServerDiscoverService">
        <operation name="hello">
            <input message="tns:hello"/>
            <output message="tns:helloResponse"/>
        </operation>
    </portType>
    <binding name="ServerDiscoverServicePortBinding" type="tns:ServerDiscoverService">
        <wsp:PolicyReference URI="#ServerDiscoverServicePortBindingPolicy"/>
        <operation name="hello">
            <input/>
            <output/>
        </operation>
    </binding>
    <service name="ServerPollingService">
        <port name="ServerDiscoverServicePort" binding="tns:ServerDiscoverServicePortBinding"/>
    </service>
    <wsp:Policy wsu:Id="ServerDiscoverServicePortBindingPolicy">
        <wsp:ExactlyOne>
            <wsp:All>
                <wsam:Addressing/>
                <wsrm:RMAssertion/>
            </wsp:All>
        </wsp:ExactlyOne>
    </wsp:Policy>
</definitions>
